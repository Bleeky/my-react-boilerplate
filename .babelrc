{
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./src"],
      }
    ],
    "react-hot-loader/babel", [
      "transform-runtime",
      {
        "polyfill": false,
        "regenerator": true,
      }
    ],
    "transform-react-inline-elements",
  ],
  "presets": [
    "env", [
      "latest",
      {
        "es2015": {
          "modules": false,
        },
      },
    ],
    "react",
    "stage-0",
  ],
}
